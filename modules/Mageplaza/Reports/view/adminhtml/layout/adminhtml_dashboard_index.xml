<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="styles"/>
    <head>
        <css src="Mageplaza_Core/css/font-awesome.min.css"/>
        <css src="Mageplaza_Reports/css/dashboard/custom.css"/>
    </head>
    <body>
        <referenceContainer name="content">
            <block class="Mageplaza\Reports\Block\SwitchToNew" name="switch_to_ar_dashboard" before="-"/>
            <block class="Magento\Backend\Block\Template" name="first_time_popup" ifconfig="mageplaza_reports/general/first_time_install" template="Mageplaza_Reports::popup.phtml"/>
        </referenceContainer>
    </body>
</page>