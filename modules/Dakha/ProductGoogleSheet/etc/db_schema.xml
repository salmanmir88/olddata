<?xml version="1.0"?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
    <table name="product_google_sheets" resource="default" engine="innodb" comment="Product google sheets">
        <column xsi:type="int" name="sheet_id" padding="10" identity="true" unsigned="true" nullable="false" comment="Sheet id"/>
        <column xsi:type="varchar" name="sheet_name" nullable="true" comment="Shee name"/>
        <constraint xsi:type="primary" referenceId="PRIMARY">
            <column name="sheet_id"/>
        </constraint>
    </table>
</schema>        