<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">

    <default>
        <carriers>
            <fetchr>
                <productbagcount>0</productbagcount>
                <active>1</active>
                <model>Fetchr_Shipping_Model_Carrier</model>
                <title>Fetchr</title>
                <sort_order>10</sort_order>
                <price>10.00</price>
                <!-- <sallowspecific>0</sallowspecific> -->
            </fetchr>
        </carriers>
        <fetchr_shipping>
            <settings>
                <liveurl>https://api.order.fetchr.us</liveurl>
                <stagingurl>https://spawn.stag.fetchr.us</stagingurl>
                <order_status_update>*/10 * * * *</order_status_update>
            </settings>
        </fetchr_shipping>
    </default>
</config>
