<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::marketing">
                    <resource id="Amasty_Affiliate::affiliate" title="Affiliate" translate="title" sortOrder="10">
                        <resource id="Amasty_Affiliate::program" title="Manage Programs" translate="title" sortOrder="10" />
                        <resource id="Amasty_Affiliate::account" title="Manage Accounts" translate="title" sortOrder="20" />
                        <resource id="Amasty_Affiliate::banner" title="Manage Banners" translate="title" sortOrder="30" />
                        <resource id="Amasty_Affiliate::transaction" title="Manage Transactions" translate="title" sortOrder="40" />
                        <resource id="Amasty_Affiliate::withdrawals" title="Manage Withdrawals" translate="title" sortOrder="50" />
                        <resource id="Amasty_Affiliate::settings" title="Settings" translate="title" sortOrder="60" />
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
